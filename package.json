{
  "name": "express-ts",
  "version": "1.1.2",
  "author": "Dipadana Putu",
  "license": "ISC",
  "description": "Just basic password manager",
  "engines": {
    "npm": ">=10",
    "node": ">=20"
  },
  "packageManager": "npm@9.6.6",
  "scripts": {
    "start": "npm run build && node build/src/index.js",
    "build": "rimraf ./build && tsc",
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "test:env": "rimraf ./build && dotenv -e .env.test -- npx prisma migrate reset --force && dotenv -e .env.test -- npm run dev",
    "test:run": "dotenv -e .env.test -- vitest run",
    "test": "concurrently --kill-others \"npm run test:env\" \"wait-on http://localhost:3030 && npm run test:run\"",
    "ncu": "ncu",
    "ncu:exe": "ncu -u"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "argon2": "^0.31.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/crypto-js": "^4.1.2",
    "@types/express": "^4.17.18",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.7.0",
    "@types/superagent": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "concurrently": "^8.2.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "npm-check-updates": "^16.14.4",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1",
    "rimraf": "^5.0.4",
    "superagent": "^8.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^0.34.5",
    "wait-on": "^7.0.1"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
