{
  "name": "password-manager",
  "version": "1.2.6",
  "author": "Dipadana Putu",
  "license": "ISC",
  "description": "Just basic password manager",
  "engines": {
    "npm": ">=10",
    "node": ">=20"
  },
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "start": "npm run build && node build/src/index.js",
    "build": "rimraf ./build && tsc",
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "test:env": "rimraf ./build && dotenv -e .env.test -- npx prisma migrate reset --force && dotenv -e .env.test -- npm run dev",
    "test:run": "dotenv -e .env.test -- vitest run",
    "test": "concurrently --kill-others \"npm run test:env\" \"wait-on http://localhost:3030 && npm run test:run\"",
    "ncu": "ncu",
    "ncu:u": "ncu -u"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.2.0",
    "@node-rs/argon2": "^2.0.0",
    "@prisma/client": "^5.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.16.12",
    "@types/superagent": "^8.1.9",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "concurrently": "^9.0.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "npm-check-updates": "^17.1.3",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0",
    "rimraf": "^6.0.1",
    "superagent": "^10.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vitest": "^2.1.2",
    "wait-on": "^8.0.1"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
