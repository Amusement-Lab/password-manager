{
  "name": "password-manager",
  "version": "1.2.0",
  "author": "Dipadana Putu",
  "license": "ISC",
  "description": "Just basic password manager",
  "engines": {
    "npm": ">=10",
    "node": ">=20"
  },
  "packageManager": "npm@10.2.4",
  "scripts": {
    "start": "npm run build && node build/src/index.js",
    "build": "rimraf ./build && tsc",
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "test:env": "rimraf ./build && dotenv -e .env.test -- npx prisma migrate reset --force && dotenv -e .env.test -- npm run dev",
    "test:run": "dotenv -e .env.test -- vitest run",
    "test": "concurrently --kill-others \"npm run test:env\" \"wait-on http://localhost:3030 && npm run test:run\"",
    "ncu": "ncu",
    "ncu:u": "ncu -u"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "argon2": "^0.40.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.25",
    "@types/superagent": "^8.1.4",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "npm-check-updates": "^16.14.15",
    "prettier": "^3.2.5",
    "prisma": "^5.10.2",
    "rimraf": "^5.0.5",
    "superagent": "^8.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.5",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.3.1",
    "wait-on": "^7.2.0"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
