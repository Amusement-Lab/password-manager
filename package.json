{
  "name": "password-manager",
  "version": "1.2.8",
  "author": "Dipadana Putu",
  "license": "ISC",
  "description": "Just basic password manager",
  "engines": {
    "pnpm": ">=9",
    "node": ">=22"
  },
  "packageManager": "pnpm@9.13.2",
  "scripts": {
    "start": "pnpm run build && node build/src/index.js",
    "build": "rimraf ./build && tsc",
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "test:env": "rimraf ./build && dotenv -e .env.test -- pnpm dlx prisma migrate reset --force && dotenv -e .env.test -- pnpm dev",
    "test:run": "dotenv -e .env.test -- vitest run",
    "test": "concurrently --kill-others \"pnpm test:env\" \"wait-on http://localhost:3030 && pnpm test:run\"",
    "ncu": "ncu",
    "ncu:u": "ncu -u",
    "ncu:i": "ncu -i",
    "build:swc": "rimraf ./build && swc src --out-dir build",
    "start:swc": "node build/src/index.js",
    "test:env:swc": "rimraf ./build && dotenv -e .env.test -- pnpm dlx prisma migrate reset --force && dotenv -e .env.test -- pnpm build:swc && dotenv -e .env.test -- pnpm start:swc",
    "test:swc": "concurrently --kill-others \"pnpm test:env:swc\" \"wait-on http://localhost:3030 && pnpm test:run\""
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.2.0",
    "@node-rs/argon2": "^2.0.0",
    "@prisma/client": "^5.22.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@swc/cli": "^0.5.0",
    "@swc/core": "^1.9.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/superagent": "^8.1.9",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "concurrently": "^9.1.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "npm-check-updates": "^17.1.11",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "rimraf": "^6.0.1",
    "superagent": "^10.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vitest": "^2.1.5",
    "wait-on": "^8.0.1"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
